{
  "app": {
    "bundleName": "com.huawei.movie",
    "vendor": "huawei",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {
    "default": {
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "reqPermissions": [
      {
        "name": "com.huawei.movie.DataAbilityShellProvider.PROVIDER"
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.SET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.INTERNET"
      }
    ],
    "package": "com.huawei.movie",
    "name": ".MyApplication",
    "mainAbility": "com.huawei.movie.ability.MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.huawei.movie.ability.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "permissions": [
          "com.huawei.movie.ability.DataAbility.DATA"
        ],
        "name": "com.huawei.movie.ability.DataAbility",
        "icon": "$media:icon",
        "description": "$string:dataability_description",
        "type": "data",
        "uri": "dataability://com.huawei.movie.ability.DataAbility"
      },
      {
        "orientation": "unspecified",
        "name": "com.huawei.movie.ability.DetailAbility",
        "icon": "$media:icon",
        "description": "$string:detailability_description",
        "label": "$string:entry_DetailAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.huawei.movie.ability.PlayAbility",
        "icon": "$media:icon",
        "description": "$string:playability_description",
        "label": "$string:entry_PlayAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.huawei.movie.ability.SearchAbility",
        "icon": "$media:icon",
        "description": "$string:searchability_description",
        "label": "$string:entry_SearchAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.huawei.movie.ability.UserAbility",
        "icon": "$media:icon",
        "description": "$string:userability_description",
        "label": "$string:entry_UserAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.huawei.movie.ability.EditAbility",
        "icon": "$media:icon",
        "description": "$string:editability_description",
        "label": "$string:entry_EditAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.huawei.movie.ability.LoginAbility",
        "icon": "$media:icon",
        "description": "$string:loginability_description",
        "label": "$string:entry_LoginAbility",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.NoTitleBar"
        }
      ]
    }
  }
}