                    ohos:height="match_content"
                    ohos:width="match_content"/>
                <Image
                    ohos:left_margin="$string:container_padding_size"
                    ohos:scale_mode="stretch"
                    ohos:image_src="$media:icon_arrow"
                    ohos:height="$string:middle_icon_size"
                    ohos:width="$string:middle_icon_size"/>
            </DirectionalLayout>
            <DirectionalLayout
                ohos:background_element="$color:page_background_color"
                ohos:height="$string:button_border_size"
                ohos:width="match_parent"/>
        </DirectionalLayout>

        <DirectionalLayout
            ohos:orientation="vertical"
            ohos:height="match_content"
            ohos:width="match_parent">
            <DirectionalLayout
                ohos:top_padding="$string:normal_font_size"
                ohos:bottom_padding="$string:normal_font_size"
                ohos:alignment="vertical_center"
                ohos:orientation="horizontal"
                ohos:height="match_content"
                ohos:width="match_parent">
                <Text
                    ohos:text_size="$string:normal_font_size"
                    ohos:weight="1"
                    ohos:text="$string:user_edit_sign"
                    ohos:height="match_content"
                    ohos:width="match_content"/>
                <Text
                    ohos:text_size="$string:normal_font_size"
                    ohos:id="$+id:user_edit_sign"
                    ohos:height="match_content"
                    ohos:width="match_content"/>
                <Image
                    ohos:left_margin="$string:container_padding_size"
                    ohos:scale_mode="stretch"
                    ohos:image_src="$media:icon_arrow"
                    ohos:height="$string:middle_icon_size"
                    ohos:width="$string:middle_icon_size"/>
            </DirectionalLayout>
            <DirectionalLayout
                ohos:background_element="$color:page_background_color"
                ohos:height="$string:button_border_size"
                ohos:width="match_parent"/>
        </DirectionalLayout>

        <DirectionalLayout
            ohos:orientation="vertical"
            ohos:height="match_content"
            ohos:width="match_parent">
            <DirectionalLayout
                ohos:top_padding="$string:normal_font_size"
                ohos:alignment="vertical_center"
                ohos:orientation="horizontal"
                ohos:height="match_content"
                ohos:width="match_parent">
                <Text
                    ohos:text_size="$string:normal_font_size"
                    ohos:weight="1"
                    ohos:text="$string:user_edit_region"
                    ohos:height="match_content"
                    ohos:width="match_content"/>
                <Text
                    ohos:text_size="$string:normal_font_size"
                    ohos:id="$+id:user_edit_region"
                    ohos:height="match_content"
                    ohos:width="match_content"/>
                <Image
                    ohos:left_margin="$string:container_padding_size"
                    ohos:scale_mode="stretch"
                    ohos:image_src="$media:icon_arrow"
                    ohos:height="$string:middle_icon_size"
                    ohos:width="$string:middle_icon_size"/>
            </DirectionalLayout>
        </DirectionalLayout>

    </DirectionalLayout>

    <Button
        ohos:background_element="$graphic:background_logout"
        ohos:bottom_margin="$string:container_padding_size"
        ohos:text_color="$color:button_text_color"
        ohos:text_size="$string:normal_font_size"
        ohos:text="$string:user_edit_logout"
        ohos:id="$+id:logout"
        ohos:height="$string:button_height_size"
        ohos:width="match_parent"/>
</DirectionalLayout>